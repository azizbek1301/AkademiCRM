        {
          "Host": "localhost",
          "Port": 5256
        }
      ],
      "UpstreamPathTemplate": "/allEvent",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/Event/Create",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5256
        }
      ],
