        {
          "Host": "localhost",
          "Port": 5256
        }
      ],
      "UpstreamPathTemplate": "/createAdmin",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/api/Admin/Delete?id={Id}",
