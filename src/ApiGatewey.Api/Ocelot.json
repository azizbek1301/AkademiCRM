        {
          "Host": "localhost",
          "Port": 5256
        }
      ],
      "UpstreamPathTemplate": "/allAdmin",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/Admin/Create",
      "DownstreamScheme": "http",
