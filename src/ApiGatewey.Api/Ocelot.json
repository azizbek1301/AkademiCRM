{
  "Routes": [
    // Admin
    {
      "DownstreamPathTemplate": "/api/Admin/GetAll",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5256
        }
      ],
      "UpstreamPathTemplate": "/allAdmin",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/Admin/Create",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5256
        }
      ],
      "UpstreamPathTemplate": "/createAdmin",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/api/Admin/Delete?id={Id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5256
        }
      ],
      "UpstreamPathTemplate": "/deleteAdmin/{Id}",
      "UpstreamHttpMethod": [ "Delete" ]
    },
    {
      "DownstreamPathTemplate": "/api/Admin/GetById?id={Id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5256
        }
      ],
      "UpstreamPathTemplate": "/ById/{Id}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/Admin/Update",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5256
        }
      ],
      "UpstreamPathTemplate": "/update",
      "UpstreamHttpMethod": [ "Put" ]
    },


    

  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000"
  }
}